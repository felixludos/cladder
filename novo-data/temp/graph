// Causal Model
digraph {
	"Market Demand" [label="Market Demand" color=lightblue shape=diamond style=filled]
	"Technology Implementation" [label="Technology Implementation" color=lightpink shape=ellipse style=filled]
	"Management Quality" [label="Management Quality" color=lightblue shape=diamond style=filled]
	"Training & Development" [label="Training & Development" color=lightpink shape=ellipse style=filled]
	"Employee Engagement" [label="Employee Engagement" color=lightyellow shape=box style=filled]
	"Process Optimization" [label="Process Optimization" color=lightyellow shape=box style=filled]
	"Operational Efficiency Improvement" [label="Operational Efficiency Improvement" color=lightgreen shape=ellipse style=filled]
	"Public Perception" [label="Public Perception" color=lightgray shape=box style=filled]
	"Market Demand" -> "Technology Implementation"
	"Management Quality" -> "Training & Development"
	"Training & Development" -> "Employee Engagement"
	"Management Quality" -> "Employee Engagement"
	"Technology Implementation" -> "Process Optimization"
	"Employee Engagement" -> "Process Optimization"
	"Technology Implementation" -> "Operational Efficiency Improvement"
	"Training & Development" -> "Operational Efficiency Improvement"
	"Process Optimization" -> "Operational Efficiency Improvement"
	"Operational Efficiency Improvement" -> "Public Perception"
	"Market Demand" -> "Public Perception"
}
